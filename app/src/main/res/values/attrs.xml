<resources>
    <declare-styleable name="FullscreenAttrs">
        <attr name="fullscreenBackgroundColor" format="color" />
        <attr name="fullscreenTextColor" format="color" />
    </declare-styleable>

    <declare-styleable name="CheckButton">
        <attr name="isChecked" format="boolean"/>
        <attr name="checkedIcon" format="reference" />
        <attr name="unCheckedIcon" format="reference" />
    </declare-styleable>

    <declare-styleable name="OptionItemView">
        <attr name="key" format="string"/>
        <attr name="desc" format="string" />
        <attr name="icon" format="reference" />
        <attr name="goIcon" format="reference"/>
    </declare-styleable>
</resources>